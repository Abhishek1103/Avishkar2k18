            <Cursor fx:constant="OPEN_HAND" />
         </cursor></JFXButton>
      <JFXTextArea fx:id="tagsTextArea" labelFloat="true" layoutX="48.0" layoutY="178.0" prefHeight="58.0" prefWidth="533.0" promptText="Enter Comma Delimited Tags for the selected video">
         <font>
            <Font size="15.0" />
         </font>
      </JFXTextArea>
      <JFXButton fx:id="addTagsToVideoButton" buttonType="RAISED" layoutX="219.0" layoutY="278.0" onAction="#addTagsToVideo" prefHeight="26.0" prefWidth="143.0" style="-fx-border-radius: 5; -fx-background-color: b2b2b2;" text="Add Tags">
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor></JFXButton>
      <JFXListView fx:id="videoListView" depth="1" layoutX="743.0" layoutY="168.0" prefHeight="260.0" prefWidth="209.0" showTooltip="true" style="-fx-border-radius: 7;" verticalGap="10.0">
         <effect>
            <Glow />
         </effect>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
      </JFXListView>
      <ScrollPane fx:id="scrollPane" layoutX="19.0" layoutY="458.0" prefHeight="184.0" prefWidth="949.0">
         <effect>
            <Glow />
         </effect>
      </ScrollPane>
   </children>
