<AnchorPane fx:id="addChannelAnchorPane" prefHeight="656.0" prefWidth="986.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.UI.AddChannelController">
   <children>
      <Label layoutX="48.0" layoutY="74.0" prefHeight="74.0" prefWidth="171.0" text="Enter Channel Name:">
         <font>
            <Font size="15.0" />
         </font>
         <effect>
            <Glow />
         </effect>
      </Label>
      <JFXTextField fx:id="channelNameTxt" layoutX="231.0" layoutY="98.0" prefHeight="26.0" prefWidth="546.0" promptText="Channel Name" />
      <JFXButton fx:id="createChannelButton" buttonType="RAISED" layoutX="54.0" layoutY="383.0" onAction="#createChannel" prefHeight="26.0" prefWidth="160.0" ripplerFill="WHITE" style="-fx-border-radius: 5; -fx-background-color: b2b2b2;" text="Create Channel">
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor></JFXButton>
      <JFXButton fx:id="discardButton" buttonType="RAISED" layoutX="394.0" layoutY="383.0" onAction="#discard" prefHeight="26.0" prefWidth="123.0" ripplerFill="WHITE" style="-fx-border-radius: 5; -fx-background-color: b2b2b2;" text="Discard">
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor></JFXButton>
      <JFXButton fx:id="addVideosButton" buttonType="RAISED" layoutX="819.0" layoutY="111.0" onAction="#addVideos" prefHeight="26.0" prefWidth="123.0" style="-fx-border-radius: 5; -fx-background-color: b2b2b2;" text="Add Videos">
